        <!--CosmosDb.Collection.IndexingPolicy.TableView-->
        <View>
            <Name>CosmosDb.Collection.IndexingPolicy.TableView</Name>
            <ViewSelectedBy>
                <TypeName>CosmosDb.Collection.IndexingPolicy</TypeName>
            </ViewSelectedBy>
            <TableControl>
                <TableHeaders>
                    <TableColumnHeader>
                        <Label>IndexingMode</Label>
                    </TableColumnHeader>
                    <TableColumnHeader>
                        <Label>Automatic</Label>
                    </TableColumnHeader>
                    <TableColumnHeader>
                        <Label>IncludedPaths</Label>
                    </TableColumnHeader>
                    <TableColumnHeader>
                        <Label>ExcludedPaths</Label>
                    </TableColumnHeader>
                </TableHeaders>
                <TableRowEntries>
                    <TableRowEntry>
                        <TableColumnItems>
                            <TableColumnItem>
                                <PropertyName>indexingMode</PropertyName>
                            </TableColumnItem>
                            <TableColumnItem>
                                <PropertyName>automatic</PropertyName>
                            </TableColumnItem>
                            <TableColumnItem>
                                <PropertyName>includedPaths</PropertyName>
                            </TableColumnItem>
                            <TableColumnItem>
                                <PropertyName>excludedPaths</PropertyName>
                            </TableColumnItem>
                        </TableColumnItems>
                    </TableRowEntry>
                </TableRowEntries>
            </TableControl>
        </View>
        <!--CosmosDb.Collection.IndexingPolicy.WideView -->
        <View>
            <Name>CosmosDb.Collection.IndexingPolicy.WideView</Name>
            <ViewSelectedBy>
                <TypeName>CosmosDb.Collection.IndexingPolicy</TypeName>
            </ViewSelectedBy>
            <WideControl>
                <WideEntries>
                    <WideEntry>
                        <WideItem>
                            <ScriptBlock>
                                if ($_.Automatic) { ('{0} (automatic)' -f $_.indexingMode) } else { $_.indexingMode }
                            </ScriptBlock>
                        </WideItem>
                    </WideEntry>
                </WideEntries>
            </WideControl>
        </View>
