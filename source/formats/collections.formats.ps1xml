                    <TableColumnHeader>
                        <Label>ResourceId</Label>
                    </TableColumnHeader>
                    <TableColumnHeader>
                        <Label>Timestamp</Label>
                    </TableColumnHeader>
                    <TableColumnHeader>
                        <Label>Uri</Label>
